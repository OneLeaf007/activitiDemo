<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.study.mapper.EntiyTestMapper">


    <resultMap type="com.springboot.study.entiy.EntiyTest" id="BaseResultMap">
    <id property="id" column="id"/>
    <result property="name" column="name"/>
    </resultMap>

    <select id="selectById" resultType="com.springboot.study.entiy.EntiyTest">
        SELECT * from EntiyTest where id=#{id}
    </select>



</mapper>



