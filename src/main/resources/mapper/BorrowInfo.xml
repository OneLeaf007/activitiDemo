<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.study.mapper.BorrowInfoMapper">

    <select id="selectById" resultType="com.springboot.study.entiy.BorrowInfo">
        SELECT * from borrow_info where id=#{id}
    </select>

    <select id="inster" >
        INSERT into borrow_info set borrow_name =#{borrowInfo.borrow_name}
    </select>

</mapper>



